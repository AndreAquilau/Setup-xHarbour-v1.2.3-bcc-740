#define PI 3.1415

FUNCTION Exercicio13(nRaio)

    TRY

        ? 'Area:',  (PI * nRaio) ^ 2
        
        ? 2 ** 2
        ? 2 ** 3
        ? 2 / 2
        ? 2 / 2
        ? 1 / (10 + 20)
        ? (1 - 3) / 2
        ? ((1^3) + 4 / 5) / (3**2+8)

    CATCH

        ? 'Erro Exercicio13(nRaio)'

    END

RETURN nil